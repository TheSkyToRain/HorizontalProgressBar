<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ebar="http://schemas.android.com/apk/res/com.example.administrator.myapplication"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#99cc33"
    tools:ignore="ResAuto">
    <com.example.administrator.myapplication.Views.HorizontalProgressBar
        android:id="@+id/progressbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:progress="26"
        android:padding="5dp"
        ebar:progress_text_color="#fff"
        ebar:progress_reach_color="#000000"
        android:layout_marginTop="30dp"
        />
</RelativeLayout>
